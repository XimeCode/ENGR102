#ask user for input
from math import *
first_int = int(input("Enter an integer: "))
second_int =  int(input("Enter another integer: "))

#figure out the ints between first int and second int 
#sum them 

numsum = 0 #assign variable

if first_int > second_int:
    first_int, second_int = second_int, first_int 

current = first_int
while current <= second_int:
    numsum += current
    current += 1

print(f"The sum of all integers from {first_int} to {second_int} is {numsum}")

